{
  "name": "next-starter",
  "version": "0.1.0",
  "description": "This project is a front-end web application built with Next.js and React, using JavaScript, CSS, and HTML for development.",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint:check": "eslint --ext .js,.ts,.tsx,.cjs ./",
    "eslint:fix": "eslint --fix --ext .js,.ts,.tsx,.cjs ./",
    "style:check": "prettier --check --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,html,md,cjs}\"",
    "style:fix": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,html,md,cjs}\"",
    "postinstall": "patch-package",
    "clean": "npx rimraf node_modules",
    "clean:cache": "npx rimraf node_modules/.cache",
    "npm:check": "npx npm-check-updates",
    "lint-staged": "lint-staged",
    "prepare": "if [[ $NODE_ENV != \"production\" ]]; then husky install; fi && patch-package"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.5.3",
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/eslint": "8.21.1",
    "@types/prettier": "2.7.2",
    "autoprefixer": "10.4.14",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "npm-check-updates": "16.7.12",
    "patch-package": "6.5.1",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "rimraf": "4.4.0",
    "tailwindcss": "3.2.7"
  },
  "lint-staged": {
    "*.{js,cjs,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --cache"
    ],
    "*.{md,html,json,css,less,scss,sass}": [
      "prettier --write"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funnyzak/next-starter/issues"
  },
  "homepage": "https://github.com/funnyzak/next-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funnyzak/next-starter.git"
  },
  "keywords": [
    "Next.js",
    "React",
    "JavaScript",
    "CSS",
    "HTML",
    "web development",
    "front-end development",
    "responsive design",
    "static site generation",
    "server-side rendering",
    "SEO optimization",
    "RESTful API",
    "GraphQL",
    "TypeScript",
    "Git",
    "npm",
    "ESLint",
    "Prettier"
  ],
  "author": "leon <silenceace@gmail.com>"
}
